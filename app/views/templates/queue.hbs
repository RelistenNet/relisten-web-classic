<ul>
  {{#each queue}}
    <li>
      <a class="song {{active}}" href="/{{year}}/{{month}}/{{longDay}}/{{longSlug}}">
        {{title}}
      </a>
      <span>
        [<a href="/{{year}}/{{month}}/{{longDay}}">
          {{month}}/{{day}}/{{year}}
        </a>]
      </span>
    </li>
  {{/each}}
</ul>

{{#if loggedIn}}
  <button class=save>Save Playlist</button>
{{/if}}
