<table>
   <thead>
      <tr>
         <th class="heading">Band</th>
         <th class="heading">Tapes</th>
      </tr>
   </thead>
   {{#if realMusic}}
   {{else}}
   <thead>
    <tr>
      <th><a href="/grateful-dead">Grateful Dead</a></th>
      <td>{{bands.grateful-dead.shows}}</td>
    </tr>
   </thead>
   <thead>
    <tr>
      <th><a href="/phish">Phish</a></th>
      <td>{{bands.phish.shows}}</td>
    </tr>
   </thead>
   <thead>
    <tr>
      <th>&nbsp;</th>
      <td>&nbsp;</td>
    </tr>
   </thead>
   {{/if}}
   {{#each bands}}
   <tbody>
      <tr>
        <th>
          <a href="/{{@key}}">{{name}}</a>
        </th>
        <td>{{shows}}</td>
      </tr>
   </tbody>
  {{/each}}
</table>

{{#if default}}<button class=enable-notifications>ENABLE NOTIFICATIONS</button>{{/if}}
