<div class=ul-header>{{data.year}}</div>
<ul>
{{#each data.shows}}
  <li>
    <a class="show-{{year}}-{{month}}-{{day}}" href="/{{../band}}/{{year}}/{{month}}/{{day}}">
      <span class=date>{{month}}/{{addZero day}}</span>
      <span class=venue>{{venue_name}}</span>
    </a>
  </li>
{{/each}}
</ul>
