<div class=ul-header>{{songs.title}}</div>
<ul>
  <li class=add-all>Add all</li>
  {{#each songs._songs}}
    <li data-id={{_id}} data-song="{{title}}">
      <a class=song href="/{{year}}/{{month}}/{{day}}{{#if showVersion}}-{{showVersion}}{{/if}}/{{slug}}{{#if version}}/{{version}}{{/if}}">{{title}} <span>{{toHHMMSS duration}}</span></a>
      <div class=play>·êÖ</div>
      <div title="Add To Queue" class=add>+</div>
    </li>
  {{/each}}
  <li>{{songs.source.[0]}}</li>
</ul>
